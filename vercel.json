{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/app",
      "destination": "/index.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/landing-page.html"
    },
    {
      "source": "/landing-page.html",
      "destination": "/landing-page.html"
    }
  ]
}
